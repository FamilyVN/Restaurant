<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.tuananh.restaurant.manager"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.ActionBar">
        <activity
            android:name=".view.activity.menu.MenuActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.SettingActivity"
            android:label="@string/title_settings"/>
        <activity
            android:name=".ui.activity.SellActivity"
            android:label="@string/title_sell"/>
        <activity
            android:name=".ui.activity.BoardActivity"
            android:label="@string/title_board"/>
        <activity android:name=".ui.activity.PaymentActivity"/>
    </application>

</manifest>